{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": "src",
    "types": [
      "astro/client",
      "node/types"
    ],
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@templates/*": [
        "templates/*"
      ],
      "@pages/*": [
        "pages/*"
      ]
    }
  }
}